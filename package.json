{
	"name": "figma-plugin-starter-radix-ui",
	"author": "Scott Baggett <hello@scottbaggett.com>",
	"version": "1.0.0",
	"description": "Figma plugin starter with Vite and Radix Themes",
	"type": "module",
	"scripts": {
		"dev": "pnpm build && pnpm dev:watch",
		"dev:watch": "concurrently \"pnpm build:plugin --watch\" \"pnpm build:ui --watch\"",
		"build": "pnpm build:plugin && pnpm build:ui && pnpm build:manifest",
		"build:plugin": "vite build --mode plugin",
		"build:ui": "vite build",
		"build:manifest": "cp manifest.json dist/manifest.json",
		"typecheck": "tsc --noEmit",
		"format": "biome format src --write"
	},
	"keywords": [
		"figma",
		"plugin",
		"vite",
		"radix",
		"react"
	],
	"license": "MIT",
	"packageManager": "pnpm@10.20.0",
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/themes": "^3.2.1",
		"date-fns": "^4.1.0",
		"lucide-react": "^0.555.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-hook-form": "^7.67.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@figma/plugin-typings": "^1.121.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"concurrently": "^9.2.1",
		"typescript": "^5.9.3",
		"vite": "^7.2.4",
		"vite-plugin-singlefile": "^2.3.0"
	}
}
